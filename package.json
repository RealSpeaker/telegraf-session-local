{
  "name": "telegraf-session-local",
  "version": "2.1.0",
  "description": "Telegraf local sessions middleware",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RealSpeaker/telegraf-session-local.git"
  },
  "keywords": [
    "telegram",
    "telegram bot",
    "telegraf",
    "telegraf session",
    "telegraf session local",
    "session",
    "middleware",
    "database",
    "local database",
    "json"
  ],
  "author": "Tema Smirnov <npm.tema@smirnov.one>",
  "license": "MIT",
  "contributors": [
    "EdJoPaTo <telegraf-session-local-npm-package@edjopato.de> (https://edjopato.de/)"
  ],
  "bugs": {
    "url": "https://github.com/RealSpeaker/telegraf-session-local/issues"
  },
  "homepage": "https://realspeaker.github.io/telegraf-session-local/",
  "files": [
    "dist",
    "!*.test.*"
  ],
  "scripts": {
    "build": "del-cli dist && tsc",
    "prepack": "npm run build",
    "lint": "xo",
    "test": "tsc --sourceMap && xo && nyc mocha tests --require should",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "release": "np"
  },
  "type": "commonjs",
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "debug": "^4.0.0",
    "steno": "^1.0.0"
  },
  "peerDependencies": {
    "telegraf": "^4.0.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^1.0.2",
    "@types/debug": "^4.1.5",
    "@types/node": "^14.14.31",
    "coveralls": "^3.1.0",
    "del-cli": "^4.0.0",
    "mocha": "^9.0.2",
    "np": "*",
    "nyc": "^15.1.0",
    "should": "^13.2.3",
    "telegraf": "^4.0.2",
    "typescript": "^4.0.2",
    "xo": "^0.42.0"
  },
  "main": "dist",
  "types": "dist",
  "nyc": {
    "all": true,
    "include": [
      "dist",
      "source"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "@typescript-eslint/no-this-alias": "off",
      "@typescript-eslint/no-unsafe-member-access": "off",
      "arrow-parens": "off",
      "brace-style": "off",
      "camelcase": "off",
      "capitalized-comments": "off",
      "comma-dangle": "off",
      "curly": "off",
      "eslint-comments/no-unused-disable": "warn",
      "func-names": "off",
      "import/extensions": "off",
      "import/order": "off",
      "max-statements-per-line": "off",
      "new-cap": "off",
      "no-else-return": "off",
      "no-promise-executor-return": "off",
      "no-undef": "warn",
      "no-unused-vars": "off",
      "node/no-mixed-requires": "off",
      "object-curly-spacing": "off",
      "object-shorthand": "off",
      "one-var": "off",
      "padded-blocks": "off",
      "padding-line-between-statements": "off",
      "prefer-destructuring": "off",
      "prefer-object-spread": "off",
      "prefer-rest-params": "off",
      "promise/prefer-await-to-then": "off",
      "space-before-function-paren": "off",
      "unicorn/empty-brace-spaces": "off",
      "unicorn/error-message": "off",
      "unicorn/filename-case": "off",
      "unicorn/no-this-assignment": "off",
      "unicorn/prefer-module": "off",
      "unicorn/prefer-node-protocol": "off",
      "unicorn/prevent-abbreviations": "off"
    }
  },
  "np": {
    "yarn": false
  }
}
